generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Program {
  id      Int      @id @default(autoincrement())
  name    String
  clients Client[] // Implicit many-to-many
}

model Client {
  id       Int      @id @default(autoincrement())
  name     String
  age      Int
  gender   String
  programs Program[] // Implicit many-to-many
}

